  "dependencies": {
    "apollo-server-express": "^2.0.6",
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "express-opentracing": "^0.1.1",
    "graphql": "^14.0.2",
    "graphql-extensions": "^0.1.3",
    "jaeger-client": "^3.12.0",
    "knex": "^0.15.2",
    "objection": "^1.2.6"
