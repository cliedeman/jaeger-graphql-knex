  "scripts": {
    "prepare": "patch-package",
    "prettier": "prettier --write \"src/**/*.js\"",
    "prettier:list": "prettier --list-different \"src/**/*.js\"",
    "start": "nodemon --exec ts-node ./src/index.ts",
    "start:debug": "node --inspect ./node_modules/.bin/ts-node ./src/index.ts",
    "start:debug:ndb": "ndb ./node_modules/.bin/ts-node ./src/index.ts",
    "build": "tsc"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "arrowParens": "always",
    "bracketSpacing": false
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/graphql": "^14.0.0",
    "jest": "^23.5.0",
    "nodemon": "^1.18.4",
    "patch-package": "^5.1.1",
    "prettier": "^1.14.2",
    "ts-jest": "^23.1.4",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.3"
  }
}
